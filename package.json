{
  "name": "electra-bay",
  "version": "0.0.0",
  "description": "Buy ad sell anything, in ECAs.",
  "license": "MIT",
  "private": true,
  "main": "build/index.js",
  "engines": {
    "node": "9.4.0",
    "npm": "5.6.0"
  },
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "build:dev": "webpack --config webpack.dev.js",
    "clear:cache": "node tasks/redisFlushAll",
    "postinstall": "npm run build",
    "start": "node build/index.js",
    "start:dev": "npm run watch",
    "test": "mocha -r ts-node/register src/**/*.spec.ts",
    "validate": "npm run validate:lint && npm run validate:tsc",
    "validate:lint": "tslint -p tsconfig.json",
    "validate:tsc": "tsc --noEmit --pretty",
    "watch": "concurrently \"npm run watch:build\" \"npm run watch:start\"",
    "watch:build": "webpack --config webpack.dev.js --watch",
    "watch:start": "nodemon --config nodemon.json build/index.js"
  },
  "dependencies": {
    "@inspired-beings/log": "^1.0.8",
    "@sendgrid/mail": "^6.2.1",
    "@types/connect-flash": "0.0.34",
    "@types/dotenv": "^4.0.2",
    "@types/mongodb": "^3.0.5",
    "@types/mongoose": "^5.0.2",
    "@types/multer": "^1.3.6",
    "@types/node": "^9.4.6",
    "@types/passport": "^0.4.2",
    "@types/passport-local": "^1.0.32",
    "@types/passport-local-mongoose": "^4.0.11",
    "@types/ramda": "^0.25.18",
    "@types/redis": "^2.8.5",
    "@types/sharp": "^0.17.6",
    "@types/uuid": "^3.4.3",
    "await-to-js": "^2.0.1",
    "awesome-typescript-loader": "^3.4.1",
    "aws-sdk": "^2.201.0",
    "clean-webpack-plugin": "^0.1.18",
    "connect-flash": "^0.1.1",
    "dotenv": "^5.0.0",
    "electra-js": "0.0.2",
    "email-validator": "^1.1.1",
    "lexpress": "^0.37.0",
    "moment": "^2.20.1",
    "mongodb": "^3.0.2",
    "mongoose": "^5.0.5",
    "multer": "^1.3.0",
    "nodemon": "^1.14.12",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^4.5.0",
    "ramda": "^0.25.0",
    "redis": "^2.8.0",
    "sharp": "^0.19.0",
    "ts-node": "^4.1.0",
    "tslint": "^5.9.1",
    "typescript": "^2.7.1",
    "uuid": "^3.2.1",
    "webpack": "^3.11.0",
    "webpack-merge": "^4.1.1",
    "webpack-node-externals": "^1.6.0"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.48",
    "concurrently": "^3.5.1",
    "mocha": "^5.0.1",
    "wtfnode": "^0.6.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Electra-project/Electra-Bay.git"
  },
  "keywords": [],
  "author": "",
  "bugs": {
    "url": "https://github.com/Electra-project/Electra-Bay/issues"
  },
  "homepage": "https://github.com/Electra-project/Electra-Bay#readme"
}
